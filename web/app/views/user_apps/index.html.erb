<h1>利用者一覧</h1>

<table class="table">
  <thead>
    <tr>
      <th>ID</th>
      <th>App SID</th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @user_apps.each do |user_app| %>
      <tr>
        <td><%= user_app.id%></td>
        <td><%= user_app.account_sid %></td>
        <td><%= button_delete '削除', user_app %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= paginate @user_apps %>

